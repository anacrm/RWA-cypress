{
  "name": "rwa-cypress",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "cypress": "./node_modules/.bin/cypress open",
    "e2e": "cypress open --e2e --browser chrome",
    "triggerAllTests-chrome": "npx cypress run --browser chrome",
    "triggerAllTests-headless": "npx cypress run",
    "cleanup": "rm -fr cypress/reports",
    "merge_reports": "npx mochawesome-merge cypress/reports/mochawesome-report/*.json > cypress/reports/output.json",
    "mochawesome_report": "marge --reportDir cypress/reports/mochawesome-html-report -t Cypress_Test_Report -p Cypress_HMTL_Report -f Cypress_HMTL_Report --charts true cypress/reports/output.json",
    "cy_dashboard": "npx cypress run --record --key 9ecab00c-f0a1-4a9d-b818-ad1626e01b25 || exit 0",
    "cy:report": "npm run cleanup && npm run cy_dashboard && npm run merge_reports && npm run mochawesome_report"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anacrm/RWA-cypress.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/anacrm/RWA-cypress/issues"
  },
  "homepage": "https://github.com/anacrm/RWA-cypress#readme",
  "devDependencies": {
    "cypress": "^10.8.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-multi-reporters": "^1.6.0",
    "cypress-xpath": "^1.6.2",
    "mocha": "^10.0.0",
    "mocha-junit-reporter": "^2.0.2",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.2.0"
  },
  "dependencies": {
    "chai": "^4.3.6",
    "dotenv": "^16.0.1"
  }
}